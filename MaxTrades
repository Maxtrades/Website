         <script>
             
             var videoPlayer;

             function onYouTubeIframeAPIReady() {
                 videoPlayer = new YT.Player('watch-video-iframe',
                     {
                         videoId: 'XVJHaqWs3FY',
                         events: {
                             'onReady': onPlayerReady,
                         },
                         origin: 'www.maxtrades.co.uk'
                     });
             }

             function onPlayerReady(event) {
                 event.target.setVolume(100);
             }


             $("#watch-video").on('open.zf.reveal',
                 function () {
                     if (typeof videoPlayer !== 'undefined') {
                         videoPlayer.playVideo();
                     }
                 });
             $("#watch-video").on('closed.zf.reveal',
                 function () {
                     if (typeof videoPlayer !== 'undefined') {
                         videoPlayer.pauseVideo();
                    }
                 });
         </script>

