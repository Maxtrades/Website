        <script>
            var animateSupport = "support-box";
            var waitingForSupportSection = true;
            $(window).scroll(function () {
                if (waitingForSupportSection && ($("#animate-support-section").isOnScreen()) || ($("#support-section")).isOnScreen()) {
                    waitingForSupportSection = false;
                    $("#support-section").addClass(animateSupport);
                    $(".side-buttons").addClass("animate-out");
                    //$('.sticky').foundation('_calc', true);
                } else if (!waitingForSupportSection && $("#support-section").hasClass(animateSupport) && !$("#close-support-section").isOnScreen() && !$("footer").isOnScreen()) {
                    waitingForSupportSection = true;
                    $("#support-section").removeClass(animateSupport);
                    $(".side-buttons").removeClass("animate-out");
                    //$('.sticky').foundation('_calc', true);
                }
            });

            $(window).resize(function () {
                setSupportIconPosition(); // reposition the support icon when window scroll occurs
            });
            $(document).ready(function () {
                setSupportIconPosition();
            });
        </script>
        <script>
            function CookieBannerReappear() { $(".sticky-cookie").removeClass("is-hidden"); $('.cookie-banner').removeClass("cookie-out is-hidden") }
            if (KeyValueStore.get("_cookies_usage_", false) == null) {
                sendAnalyticsEvent('MarketingCookiesConsent', 'ShowBannerUnique');
                CookieBannerReappear();
            }
            $(".cookie-button").click(removeCookieBanner);
            $("#cookies-settings-send-button").click(removeCookieBanner);
            $('a[data-open="cookie-settings-modal"]').click(function() {
                sendAnalyticsEvent('MarketingCookiesConsent', 'Settings');
            });
            function removeCookieBanner() {
                KeyValueStore.set("_cookies_usage_", true);
                var checkbox = $("#toggleMarketingCookieSettingCheckBox");
                if (checkbox.length === 0 || checkbox[0].checked) {
                    KeyValueStore.set("_mccos_", true);
                    sendAnalyticsEvent('MarketingCookiesConsent', 'Accept');
                    fireTrackingIfConsent();
                } else {
                    KeyValueStore.set("_mccos_", false);
                    sendAnalyticsEvent('MarketingCookiesConsent', 'Unchecked' );
                }
                sendAnalyticsEvent('MarketingCookiesConsent', 'Save');
                $('.sticky-cookie').addClass("sticky-off");
                $('.cookie-banner').addClass("cookie-out").one("webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend",
                    function() {
                        $(this).addClass("is-hidden");
                        if (setSupportIconPosition != null) setSupportIconPosition();
                        if (setFooterPaddingBottom != null) setFooterPaddingBottom();
                    });
                }
        </script>



            <script>
                (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
                })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

                ga('create', 'UA-7008980-1', 'auto');
                ga('send', 'pageview');
            </script>
            <script>fireTrackingIfConsent();</script>
        </body>
</html>
