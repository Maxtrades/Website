    <script>
        var waitingForNumbers = true;       
        $(window).scroll(function() {
            if (waitingForNumbers && $("#run-numbers").isOnScreen()) {
                waitingForNumbers = false;
                $('.count').each(function() {
                    var number = $(this);
                    var more = $(".more[data-index='" + number.data("index") + "']");
                    more.addClass('invisible'); // built-in foundation class. fixing unwanted move after number appears
                    jQuery({ Counter: number.text() / 4 }).animate({ Counter: number.text() },
                    {
                        duration: 2000,
                        easing: 'swing',
                        step: function(i) {
                            number.text(Math.ceil(i));
                        },
                        complete: function () {
                            more.hide().removeClass('invisible').fadeIn(600);
                        }
                    });
                });
            }
        });
    </script>

    <script type="text/javascript">
        var platformsWrapper = $('.platforms'),
            platformsLength = platformsWrapper.children('.item').length,
            platformsDivClasses;
        switch (platformsLength) {
            case 6:
                platformsDivClasses = 'small-up-2 medium-up-3 large-up-6 platforms-6';
                break;
            case 5:
                platformsDivClasses = 'small-up-2 medium-up-3 large-up-5 platforms-5';
                break;
            case 4:
                platformsDivClasses = 'small-up-2 medium-up-4 large-up-4 platforms-4';
                break;
            
            default:
                platformsDivClasses = 'small-up-2 medium-up-3 large-up-5 platforms-5'
                break;
        }
        platformsWrapper.attr('class', 'row ' + platformsDivClasses + ' platforms');
    </script>

