        <script>
        
        var EPCbannerElement = $('.epc-section.hide-for-medium');
        var isEPCbannerVisible = EPCbannerElement.length === 1 ? true : false;

        function fixEPCbannerMargin () {
            $headerHeight = $('.header-for-small').parent().outerHeight();
            EPCbannerElement.css({ marginTop: $headerHeight });
        };

        if (isEPCbannerVisible) fixEPCbannerMargin();
